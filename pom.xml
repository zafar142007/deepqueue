	<modelVersion>4.0.0</modelVersion>
	<groupId>com.zafar</groupId>
	<artifactId>deepqueue</artifactId>
	<packaging>war</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>deepqueue</name>
	<properties>
